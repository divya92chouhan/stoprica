:scss
  $height: 5rem;

  .page-content {
    margin-bottom: $height;
    overflow: visible;
  }

  .banner {
    position: fixed;
    bottom: 0;
    width: 99%;
    height: $height;
    background: whitesmoke;
    z-index: 1000000;
    margin: 0;
    padding: 0;
    overflow: hidden;

    .columns {
      height: $height;
      display: flex;
      align-items: center;
      justify-content: center;
      text-decoration: none;

      .column {
        min-width: 8rem;
      }

      h5 {
        margin: 0.25rem 0;
      }

      img {
        height: $height - 1.5rem;
        margin-right: 0.5rem;
      }
    }
  }
